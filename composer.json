{
    "name": "secretwebmaster/wncms-core",
    "description": "Wncms core package",
    "keywords": [
        "wncms",
        "laravel",
        "cms",
        "secretwebmaster"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "secretwebmaster/laravel-localization": "^2.3.0.6",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-permission": "^6.9",
        "secretwebmaster/wncms-tags": "^1.7.0",
        "secretwebmaster/wncms-translatable": "^1.3.1",
        "secretwebmaster/laravel-optionable": "^2.0.2",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.0",
        "barryvdh/laravel-debugbar": "*",
        "laravel/tinker": "^2.9",
        "barryvdh/laravel-ide-helper": "^3.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wncms\\": "src/",
            "Wncms\\Database\\": "database/",
            "Wncms\\Database\\Factories\\": "database/factories/",
            "Wncms\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wncms\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wncms\\Tags\\TagsServiceProvider",
                "Wncms\\Providers\\WncmsServiceProvider",
                "Wncms\\Providers\\EventServiceProvider",
                "Wncms\\Providers\\HelpersProvider",
                "Wncms\\Providers\\MailServiceProvider",
                "Wncms\\Providers\\ObserverServiceProvider",
                "Wncms\\Providers\\PluginServiceProvider",
                "Wncms\\Providers\\RouteServiceProvider",
                "Wncms\\Providers\\SettingsServiceProvider",
                "Wncms\\Providers\\ThemeServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "6.0.x-dev"
        }
    },
    "authors": [
        {
            "name": "secretwebmaster",
            "email": "earnbyshare2016@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/secretwebmaster/wncms-core/issues",
        "source": "https://github.com/secretwebmaster/wncms-core"
    },
    "homepage": "https://github.com/secretwebmaster/wncms-core",
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
